[OPTIONS]

log_ratelimit: burst=12,enable=1,rate=2/second
enable: 1

[ALIASES]

network1 172.16.100.0/24
network2 172.16.200.0/24

[IPSET network1]

dc/network1

[IPSET management]

127.0.0.1/8

[RULES]

GROUP network1 -i eth0
IN ACCEPT -log nolog
IN ACCEPT -dest +network1

[group network1]

IN ACCEPT -source dc/network1 -dest dc/network1 -log nolog
IN ACCEPT -source network1 -dest network1 -log nolog

